{
    "name": "@my-engine/core",
    "version": "0.0.1",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/src/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./webgpu": {
            "types": "./dist/src/webgpu.d.ts",
            "import": "./dist/webgpu.js",
            "require": "./dist/webgpu.cjs"
        },
        "./input": {
            "types": "./dist/src/input.d.ts",
            "import": "./dist/input.js",
            "require": "./dist/input.cjs"
        },
        "./physics": {
            "types": "./dist/src/physics.d.ts",
            "import": "./dist/physics.js",
            "require": "./dist/physics.cjs"
        },
        "./ui": {
            "types": "./dist/src/ui.d.ts",
            "import": "./dist/ui.js",
            "require": "./dist/ui.cjs"
        },
        "./audio": {
            "types": "./dist/src/audio.d.ts",
            "import": "./dist/audio.js",
            "require": "./dist/audio.cjs"
        },
        "./animation": {
            "types": "./dist/src/animation.d.ts",
            "import": "./dist/animation.js",
            "require": "./dist/animation.cjs"
        },
        "./framework": {
            "types": "./dist/src/framework.d.ts",
            "import": "./dist/framework.js",
            "require": "./dist/framework.cjs"
        }
    },
    "scripts": {
        "build": "tsup && tsc --emitDeclarationOnly",
        "dev": "tsup --watch"
    },
    "devDependencies": {
        "@types/opentype.js": "^1.3.9",
        "@webgpu/types": "^0.1.68",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "opentype.js": "^1.3.4"
    }
}
